version: default:http:0.7.2

variables:
  code: ~
  headers: ~

request:
  path: "https://reqres.in/api/users"
  method: POST
  body[json]: {
    "name": "Morpheus",
    "job": "leader"
  }

  return:
    - .request.response.code: $code
    - .request.response.headers: $headers

expose: [$code, $headers]
