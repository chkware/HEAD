version: 'default:testcase:0.7.3'
describe: UserListSpec_WithVar {Page?}

variables: 
  Page: 1
  Response: ~
  Code: ~

request:
  method: GET
  path: "{$Env.server}/users?page={$Page}"

spec:
  execute: ~
  asserts:
    - {type: AssertEquals, actual: .response.code, expected: 200}

expose: 
  - .response.code
  - .response.headers
